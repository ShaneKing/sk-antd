{
  "name": "sk-antd",
  "version": "0.5.5",
  "description": "ShaneKing for AntD",
  "homepage": "https://github.com/ShaneKing/sk-antd",
  "author": {
    "name": "ShaneKing",
    "email": "github@azolla.org",
    "url": "http://shaneking.org"
  },
  "bugs": {
    "url": "https://github.com/ShaneKing/sk-antd/issues"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/Apache-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ShaneKing/sk-antd.git"
  },
  "files": [
    "dist",
    "lib",
    "public",
    "src",
    ".editorconfig",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "AntD",
    "React",
    "ShaneKing"
  ],
  "main": "lib/index.js",
  "module": "dist/index.esm.js",
  "jest": {
    "collectCoverage": true
  },
  "scripts": {
    "build": "npm run build:dist && npm run build:lib && npm run doc:build",
    "build:dist": "umi-lib build",
    "build:lib": "umi-tools build",
    "doc:build": "umi-lib doc build",
    "doc:deploy": "umi-lib doc deploy",
    "doc:dev": "umi-lib doc dev",
    "prepublishOnly": "npm run build",
    "test": "umi test && codecov"
  },
  "dependencies": {
    "antd": "3.20.5",
    "prop-types": "15.7.2",
    "sk-i18n": "0.5.5",
    "sk-js": "0.5.5",
    "sk-react": "0.5.5"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-import": "1.12.0",
    "codecov": "3.5.0",
    "umi": "2.8.12",
    "umi-library": "1.6.0",
    "umi-tools": "0.4.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  }
}
